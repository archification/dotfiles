#git
#
#use github and gitlab
cd project
git remote add github https://github.com/user/repo.git
git remote set-url --add origin https://github.com/user/repo.git
#switch to ssh keys
git remote set-url origin git@gitlab.com:archification/thing.git
#generate pull request with hub
git clone https://git(hub/lab).com/user/thing
cd thing
hub fork
git clone https://git(hub||lab).com/archification/thing
edit things in thing
git add changes
git commit -m ""
git push
hub pull-request --push --base <Owner>:<branch> --head archification:<branch>
#keep fork updated
git remote add upstream git://github.com/owner/thing.git
git fetch upstream
git pull upstream master

